<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="PrestamosServicios"
                   namespace="PrestamosServicios.Dominio"
                   default-lazy="false">
  <class name="Prestamo" table="t_prestamos">
    <id name="Id" column="id">
      <generator class="increment" />
    </id>
    <property name="Cliente" column="cliente" />
    <property name="Equipo" column="equipo" />
    <property name="Usuario" column="usuario" />
    <property name="Motivo" column="motivo" />
    <property name="Estado" column="estado" />
  </class>
</hibernate-mapping>